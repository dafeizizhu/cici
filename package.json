    "koa-logger": "^3.0.1",
    "koa-mount": "^3.0.0",
    "koa-qs": "^2.0.0",
    "koa-route": "^3.2.0",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "md5": "^2.2.1",
    "pm2": "^2.10.3",
    "request": "^2.86.0",
    "sqlite3": "^4.0.0",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^2.4.0",
